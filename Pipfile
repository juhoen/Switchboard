[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
test="python -m unittest discover tests"
build="python setup.py sdist bdist_wheel"
deploy="twine upload dist/*"

[dev-packages]
black = "*"
twine = "*"

[packages]

[requires]
python_version = "3.8"

[pipenv]
allow_prereleases = true
